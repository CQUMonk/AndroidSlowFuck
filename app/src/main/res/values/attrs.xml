<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="flowlayout">
        <attr name="flowautoselecteffect" format="boolean"/>
        <attr name="flowmaxselect" format="integer"/>
        <attr name="flowgravity">
            <enum name="left" value="-1" />
            <enum name="center" value="0" />
            <enum name="right" value="1" />
        </attr>
        <attr name="flowsingleline" format="boolean"/>
    </declare-styleable>
</resources>